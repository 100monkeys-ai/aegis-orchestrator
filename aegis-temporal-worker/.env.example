# Environment Configuration for AEGIS Temporal Worker

# Temporal Server Connection
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=aegis-agents

# Database Connection (PostgreSQL)
DATABASE_URL=postgresql://temporal:temporal@localhost:5432/aegis

# AEGIS Runtime gRPC Service
AEGIS_RUNTIME_GRPC_URL=localhost:50051

# HTTP Server Configuration
HTTP_PORT=3000
HTTP_HOST=0.0.0.0

# Logging
LOG_LEVEL=info
NODE_ENV=development

# Worker Configuration
MAX_CONCURRENT_ACTIVITY_TASK_EXECUTIONS=100
MAX_CONCURRENT_WORKFLOW_TASK_EXECUTIONS=100
