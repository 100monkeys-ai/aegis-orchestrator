{
  "name": "aegis-temporal-worker",
  "version": "0.1.0",
  "description": "AEGIS Temporal Worker - Dynamic workflow generation and execution",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "worker": "tsx src/worker.ts",
    "server": "tsx src/server.ts",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev:worker\"",
    "dev:server": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "temporal",
    "workflow",
    "aegis",
    "agent",
    "orchestration"
  ],
  "author": "100monkeys.ai",
  "license": "MIT",
  "dependencies": {
    "@temporalio/activity": "^1.10.1",
    "@temporalio/client": "^1.10.1",
    "@temporalio/worker": "^1.10.1",
    "@temporalio/workflow": "^1.10.1",
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.10",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "handlebars": "^4.7.8",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
